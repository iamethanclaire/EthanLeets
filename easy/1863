class Solution:
    def subsetXORSum(self, nums: List[int]) -> int:
        def xors(index: int, curr: int) -> int:
            if index == len(nums):
                return curr
            
            with_curr = xors(index + 1, curr ^ nums[index])
            without_curr = xors(index + 1, curr)
            return with_curr + without_curr

        return xors(0,0)
    
    
